{
  "name": "nestjs-redis-cluster",
  "version": "1.2.10",
  "description": "A nestjs redis module w/ cluster support",
  "author": "Ishmael Samuel (useparagon.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/useparagon/nestjs-redis-cluster"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "yarn run build",
    "publish:npm": "yarn publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "^6.6.7",
    "@types/ioredis": "^4.0.4",
    "@types/uuid": "^3.4.4",
    "ioredis": "^4.2.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.3.1",
    "@types/node": "^10.7.1",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.4"
  },
  "keywords": [
    "nestjs",
    "nest",
    "redis",
    "nestjs-redis"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
